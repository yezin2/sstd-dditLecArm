package com.jsp.dto;

public class MemberVO{
		private String id;
		private String pwd;
		private String email;
		private String phone;
		
		public MemberVO(String id, String pwd, String email, String phone){
			this.id = id;
			this.pwd = pwd;
			this.email = email;
			this.phone = phone;
		}
		public String takeId() {
			return this.id;
		}
	}