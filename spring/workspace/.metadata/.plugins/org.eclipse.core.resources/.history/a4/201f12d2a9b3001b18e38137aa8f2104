<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true"%>
<%!
	class MemberVO{
		private String id;
		private String pwd;
		private String email;
		private String phone;
		
		public MemberVO(String id, String pwd, String email, String phone){
			this.id = id;
			this.pwd = pwd;
			this.email = email;
			this.phone = phone;
		}
		public String takeId() {
			return this.id;
		}
	}
%>
<%
	MemberVO member = new MemberVO("mimi", "mimi", "mimi@naver.com", "010-1234-1234");
	
	pageContext.setAttribute("member", member);
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title></title>
</head>
<body>
	<h1>아이디 : ${member.takeId }</h1>
</body>
</html>