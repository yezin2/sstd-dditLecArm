from flask import Flask ,request, render_template, jsonify
from day10.mydao_emp import DaoEmp

app = Flask(__name__,static_url_path='') 

@app.route('/') 
@app.route('/list')
def list(): 
    de = DaoEmp()
    mylist = de.myselect()
    return render_template('list.html',mylist=mylist)
@app.route('/add.ajax')
def ajax_add():
